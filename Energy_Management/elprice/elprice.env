# set the time zone
TZ: Europe/Oslo

# for prices that matches your location - can be any table header found here https://www.nordpoolgroup.com/en/Market-data1/Dayahead/Area-Prices/ALL1/Hourly/?view=table
ELPRICE_LOCATION: 'tr.heim'

# where the raw elprice data is downloaded
ELPRICE_DIRECTORY: '/data/elprice'

# plots generated using the elprice data
PLOT_DIRECTORY: '/data/plot'

# sensors generated partly using the elprice data
SENSOR_DIRECTORY: '/data/sensors'
